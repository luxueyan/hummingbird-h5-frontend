{
  "restartable": "rs",
  "verbose": false,
  "env": {
    "NODE_ENV": "dev",
    "PORT": "3000"
  },
  "exec": "node --inspect mock/index.js",
  "watch": [
    "nodemon.json",
    "mock/"
  ],
  "ignore": [
    "src/**/*.spec.js",
    ".git",
    ".idea",
    "node_modules",
    "logs"
  ],
  "ext": "js"
}
